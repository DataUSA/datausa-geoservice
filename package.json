{
  "name": "geospatial-api",
  "version": "0.0.1",
  "description": "Geospatial Service API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dbgeo": "^1.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.13.3",
    "morgan": "^1.9.1",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "pg-promise": "^5.6.7",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.9.0",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.17.1",
    "eslint-plugin-react": "^6.10.0",
    "nodemon": "^1.9.2"
  }
}
